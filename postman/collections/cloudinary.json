					"listen": "prerequest",
					"script": {
						"id": "710a7e66-a8cf-4f27-a8f9-ab7cfb44e1a2",
						"exec": [
							"const crypto = pm.require('npm:crypto-js@4.2.0');",
							"",
							"const timestamp = Math.floor(Date.now() / 1000).toString();",
							"const toSign = `timestamp=${timestamp}${pm.collectionVariables.get(\"api_secret_cloud\")}`",
							"",
							"const signature = crypto.SHA1(toSign).toString(crypto.enc.Hex);",
							"",
							"pm.collectionVariables.set(\"timestamp\",timestamp);",
							"pm.collectionVariables.set(\"signature\",signature);",
							"",
							""
						],
						"type": "text/javascript",
